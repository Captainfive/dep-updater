{
    "name": "dep-updater",
    "version": "1.0.0",
    "description": "npm Dependencies Updater",
    "main": "index.js",
    "scripts": {
        "prepublishOnly": "pkg-ok",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fraxken/dep-updater.git"
    },
    "keywords": [
        "SlimIO",
        "monitoring"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/fraxken/dep-updater/issues"
    },
    "homepage": "https://github.com/fraxken/dep-updater#readme",
    "dependencies": {
        "inquirer": "^6.2.1",
        "kleur": "^3.0.1",
        "make-promises-safe": "^4.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^7.2.1",
        "@commitlint/config-conventional": "^7.1.2",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^2.0.3",
        "@types/es6-shim": "^0.31.39",
        "@types/node": "^10.12.18",
        "cross-env": "^5.2.0",
        "eslint": "^5.11.1",
        "husky": "^1.3.0",
        "jsdoc": "^3.5.5",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=10"
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    },
    "preferGlobal": true,
    "bin": {
        "depup": "./bin/index.js"
    }
}
